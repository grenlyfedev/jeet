<script>
  let url = window.location.pathname;
  let check = document.querySelector(`.van-tabbar-item[data-url='${url}']`);
  document.querySelector("#nav_checkUrl").setAttribute("data-url", url);
  if (check) check.classList.add("block-click");
  let pathname = document
    .querySelector("#nav_checkUrl")
    .getAttribute("data-url");
  let listNavs = document.querySelector(
    "#nav_checkUrl .van-hairline--top-bottom"
  ).children;
  let select = 0;
  switch (pathname) {
    case "/home":
      select = 0;
      break;
    case "/events":
      select = 1;
      break;
    case "/promotion":
      select = 2;
      break;
    case "/wallet":
      select = 3;
      break;
    case "/mian":
      select = 4;
      break;
    default:
      break;
  }

  let arr = [
    { id: 1, src: "/images2/home_yellow.png" },
    { id: 2, src: "/images2/trophy_yellow.png" },
    { id: 3, src: "/images2/diamond.png" },
    { id: 4, src: "/images2/wallet_yellow.png" },
    { id: 5, src: "/images2/account_yellow.png" },
  ];

  listNavs[select].children[1].children[0].children[0].setAttribute(
    "src",
    arr[select].src
  ); // img
  listNavs[select].children[1].children[1].classList.add("action"); // name active
</script>
