    <script>
        document.addEventListener('DOMContentLoaded', () => {
            const items = document.querySelectorAll('#scrollContainer .item');

            items.forEach(item => {
                item.addEventListener('click', () => {
                    // Reset background color for all items
                    items.forEach(i => {
                        i.classList.remove('bg-[#89E203]/[0.5]');
                        i.classList.add('bg-[#89E203]/[0.2]');
                    });

                    // Set the clicked item's background to a lighter shade
                    item.classList.remove('bg-[#89E203]/[0.2]');
                    item.classList.add('bg-[#89E203]/[0.5]');
                });
            });
        });
    </script>
    <script>
