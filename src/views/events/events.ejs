    <script>
        $(document).ready(function () {
            function fetchMyNetworkData() {
                $.ajax({
                    type: "POST",
                    url: "/api/webapi/admin/getMyNetworkData",
                    dataType: "json",
                    success: function (response) {
                        $("#spinner").hide();
                        if (response.status) {
                            // Update the number of registered users
                            $("#number_of_registered").text(response.users_registered);
                            // Update the total deposits
                            $("#total_deposited_amount").text(response.total_deposits);
                            $("#deposited_number").text(response.deposit_number);
                            $("#making_first_deposit").text(response.making_first_deposit);
                        } else {
                            console.error("Error fetching data:", response.message);
                        }
                    },
                    error: function (error) {
                        $("#spinner").hide();
                        console.error("Error fetching data:", error);
                    }
                });
            }

            function fetchTeamNetworkData() {
                $.ajax({
                    type: "POST",
                    url: "/api/webapi/getTeamNetworkData",
                    dataType: "json",
                    success: function (response) {
                        $("#spinner").hide();
                        if (response.status) {
                            // Update the number of registered users
                            $("#number_of_registered_team").text(response.users_registered);
                            // Update the total deposits
                            $("#total_deposited_amount_team").text(response.total_deposits);
                            $("#deposited_number_team").text(response.deposit_number);
                            $("#making_first_deposit_team").text(response.making_first_deposit);
                        } else {
                            console.error("Error fetching data:", response.message);
                        }
                    },
                    error: function (error) {
                        $("#spinner").hide();
                        console.error("Error fetching data:", error);
                    }
                });
            }

            fetchMyNetworkData(); // Call fetchMyNetworkData function on document ready
            fetchTeamNetworkData(); // Call fetchTeamNetworkData function on document ready
        });
